{
  "extends": ["../../.eslintrc.json"],
  "ignorePatterns": ["!**/*", "app-icon.js", "types"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {
        "no-var": 1,
        "@typescript-eslint/no-var-requires": 1,
        "@typescript-eslint/no-explicit-any": 1,
        "@typescript-eslint/no-unused-vars": 1,
        "@typescript-eslint/ban-types": 1
      }
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ]
}
